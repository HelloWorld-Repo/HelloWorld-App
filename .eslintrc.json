{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "implicit-arrow-linebreak": ["error", "beside"],
    "linebreak-style": ["error", "windows"],
    "class-methods-use-this": "off",
    "func-names": "off"
  }
}
